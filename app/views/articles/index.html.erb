<h1>Listing articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Published At</th>
    <th>Content</th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.author %></td>
      <td><%= article.published_at %></td>
      <td><%= article.content %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New article', new_article_path %>
